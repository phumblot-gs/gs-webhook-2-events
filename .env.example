# Application
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/gs_webhook_2_events

# Security (webhook keys are per-client, stored in database)
ADMIN_API_KEY=your-admin-api-key-here

# gs-stream-api
GS_STREAM_API_URL=https://gs-stream-api-staging.fly.dev
GS_STREAM_API_TOKEN=your-gs-stream-api-token-here

# Retry job
RETRY_JOB_INTERVAL_MS=60000
RETRY_JOB_MAX_RETRIES=10

# Axiom (optional - centralized logging)
AXIOM_TOKEN=
AXIOM_DATASET=gs-dev

# Sentry (optional - error tracking)
SENTRY_DSN=
